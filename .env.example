# Bu dosyayı .env olarak kopyalayıp kendi değerlerinle doldur.

GOOGLE_API_KEY=
GOOGLE_LLM_MODEL=models/gemini-2.0-flash
GOOGLE_MAX_OUTPUT_TOKENS=512

# Hız ayarları
EMBED_PROVIDER=local          # local | gemini
HF_LIMIT=1000                 # her datasetten en fazla kaç satır alınsın (örn 1000)
EMBED_BATCH_SIZE=64           # local için efektif, gemini için istersen 8-16
EMBED_SLEEP_BETWEEN=0.0       # gemini modunda rate-limit için (örn 0.2)
